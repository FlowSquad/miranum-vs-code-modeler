{
  "name": "vs-code-bpmn-modeler",
  "displayName": "vs-code-bpmn-modeler",
  "description": "BPMN Modeler for VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:bpmn-modeler"
  ],
  "main": "./dist/src/extension.js",
  "contributes": {
    "customEditors": [
      {
        "id": "bpmn-modeler",
        "viewType": "bpmn-modeler",
        "displayName": "BPMN Modeler",
        "selector": [
          {
            "filenamePattern": "*.bpmn"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "package": "tsup --env.NODE_ENV production",
    "dev": "tsup --watch",
    "watch": "tsup",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.71.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "bpmn-js": "^10.0.0",
    "bpmn-js-properties-panel": "^1.9.0",
    "diagram-js": "^8.9.0",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "jquery": "^3.3.1",
    "mocha": "^9.2.2",
    "tsup": "^6.2.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bpmn-js-color-picker": "^0.4.0",
    "vscode-webview": "^1.0.1-beta.1"
  }
}
